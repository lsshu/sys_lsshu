<uwsgi>    
   <socket>127.0.0.1:8005</socket><!-- 内部端口，自定义 --> 
   <chdir>/home/python/kernels/</chdir><!-- 项目路径 -->            
   <wsgi-file>kernels/wsgi.py</wsgi-file>
   <master>True</master> <!-- 进程数 -->
   <processes>4</processes> <!-- 进程数 -->
   <threads>4</threads> <!-- 线程数 -->
   <daemonize>uwsgi.log</daemonize><!-- 日志文件 -->
   <pidfile>uwsgi.pid</pidfile><!-- 日志文件 -->
   <procname-prefix-spaced>django-lsshu.cn</procname-prefix-spaced><!-- 进程名称前缀 -->
   <py-autoreload>1</py-autoreload><!-- py文件修改，自动加载 -->
</uwsgi>
